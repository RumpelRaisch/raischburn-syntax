{
  "name": "raischburn-syntax",
  "theme": "syntax",
  "version": "0.2.1",
  "description": "A darker Zenburn like Syntax for Atom.",
  "keywords": [
    "syntax",
    "theme",
    "zen",
    "zenburn"
  ],
  "repository": "https://github.com/RumpelRaisch/raischburn-syntax",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "configSchema": {
    "syntaxBackground": {
      "title": "Syntax Background",
      "order": 1,
      "description": "Select a syntax background.",
      "type": "string",
      "enum": [
        "dark",
        "darker"
      ],
      "default": "dark"
    }
  },
  "main": "./lib/main"
}
